{
  "manifest_version": 3,
  "name": "Quieter",
  "version": "1.0.0",
  "description": "Quiet and cosy Twitter experience.",
  "short_name": "Quieter",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action":{
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["quieter.css"]
    }
  ]
}